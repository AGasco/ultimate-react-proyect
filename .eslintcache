\";\r\n\r\nModal.setAppElement(\"#root\");\r\n\r\nfunction GameCard({ data, curPlatforms }) {\r\n  const [modalOpen, setModalOpen] = useState(false);\r\n\r\n  const [gameData, setGameData] = useState({\r\n    name: \"\",\r\n    picture: \"\",\r\n    metacritic: \"\",\r\n    platforms: [],\r\n    esrb: \"\",\r\n    video: \"\",\r\n  });\r\n\r\n  const openModal = () => {\r\n    setModalOpen(true);\r\n  };\r\n\r\n  const closeModal = () => {\r\n    setModalOpen(false);\r\n  };\r\n\r\n  useEffect(() => {\r\n    setGameData({\r\n      ...gameData,\r\n      name: data.name,\r\n      picture: data.background_image,\r\n      metacritic: data.metacritic,\r\n      platforms: data.parent_platforms,\r\n      esrb: data.esrb_rating,\r\n      video: data.clip?.video,\r\n    });\r\n  }, [data]);\r\n\r\n  const getESRBIcon = (id) => {\r\n    if (id) {\r\n      const esrb = ESRBRatings.filter((r) => r.id === id);\r\n      if (esrb.length > 0) {\r\n        return esrb[0].icon;\r\n      }\r\n    }\r\n    return \"\";\r\n  };\r\n\r\n  return (\r\n    <div className={`gameCard ${!gameData.picture && \"hidden\"}`}>\r\n      <img\r\n        className=\"gameCard__img\"\r\n        src={gameData.picture}\r\n        alt={gameData.name + \"'s Picture\"}\r\n        onClick={openModal}\r\n      />\r\n      <div className=\"gameCard__bottom\">\r\n        <h3 className=\"gameCard__title\">{gameData.name}</h3>\r\n        <div className=\"gameCard__platforms\">\r\n          {gameData.platforms?.map((p) => {\r\n            if (curPlatforms?.includes(p.platform.id))\r\n              return <Platform key={p.platform.id} id={p.platform.id} />;\r\n          })}\r\n        </div>\r\n      </div>\r\n      <div className=\"gameCard__metacritic\">\r\n        <p\r\n          className={\r\n            gameData.metacritic >= 75\r\n              ? \"green\"\r\n              : gameData.metacritic >= 50\r\n              ? \"yellow\"\r\n              : gameData.metacritic > 0\r\n              ? \"red\"\r\n              : \"hidden\"\r\n          }\r\n        >\r\n          {gameData.metacritic}\r\n        </p>\r\n      </div>\r\n      <div className={`gameCard__ageRating ${!gameData.esrb && \"hidden\"}`}>\r\n        <a\r\n          href=\"https://en.wikipedia.org/wiki/Entertainment_Software_Rating_Board\"\r\n          target=\"_blank\"\r\n        >\r\n          <img\r\n            src={getESRBIcon(gameData.esrb?.id)}\r\n            alt={gameData.esrb?.name + \"'s icon\"}\r\n          />\r\n        </a>\r\n      </div>\r\n      <Modal isOpen={modalOpen} onRequestClose={closeModal}>\r\n        <ReactPlayer\r\n          width={\"100%\"}\r\n          height={\"100%\"}\r\n          controls={true}\r\n          playing={true}\r\n          url={`https://youtu.be/${gameData.video}`}\r\n        />\r\n      </Modal>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default GameCard;\r\n","B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\components\\Platform.jsx",[],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\components\\SearchBar.jsx",["109"],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\components\\Searchbar.jsx",["110"],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\components\\Sidebar.jsx",["111"],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\redux\\store.js",[],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\redux\\fetchActions.js",[],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\redux\\fetchReducer.js",[],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\redux\\gamesReducer.js",[],"B:\\Mis Documentos\\WebDev\\Proyectos personales\\Ultimate Project\\ultimate-react-proyect\\src\\redux\\games